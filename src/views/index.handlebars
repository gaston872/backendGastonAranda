<div>
    <h1>Productos:</h1>
    {{#each products}}
        <div class="product">
            <h2>{{title}}</h2>
            <p>{{description}}</p>
            <p>Precio: {{price}}</p>
            <p>Categoria: {{category}}</p>
            <img src="{{thumbnail}}" alt="Imagen del producto"></img>
            <p>Stock: {{stock}}</p>
        </div>
    {{/each}}
</div>

{{#each productos}}
    <h2>{{this.title}}</h2>
    <h3>{{this.description}}</h3>
    <p>{{this.price}}</p>
    <p>{{this.category}}</p>
    <p>{{this.code}}</p>
    <p>{{this.stock}}</p>
{{/each}}


